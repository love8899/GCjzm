@model object
@{
    var required = (string)ViewData["Required"];
}

@if (String.IsNullOrWhiteSpace(required))
{
    @Html.TextBoxFor(model => model, new { @class = "k-textbox", type = "password" })   //  required msg may be added by validator
}
else
{
    @Html.TextBoxFor(model => model, new { @class = "k-textbox", type = "password", data_val = "true", data_val_required = required })
}
