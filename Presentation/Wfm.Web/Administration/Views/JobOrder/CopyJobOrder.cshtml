@model JobOrderModel

@{
    ViewBag.Title = T("Admin.JobOrder.JobOrder.CopyJobOrder").Text;
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            @T("Admin.JobOrder.JobOrder.CopyJobOrder") @Html.ActionLink("(" + T("Admin.JobOrder.JobOrder.BackToList") + ")", "Index")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
             @Html.ActionLink(T("Admin.Common.Cancel").Text, "Index", "JobOrder", null, new { @class = "k-button" })
        </div>
    </div>


    @Html.Partial("_CreateOrUpdate", Model)

    @Html.Hidden("origJobOrderGuid", (Guid)ViewBag.OrigJobOrderGuid)

    
    <div class="section-header">
        <div class="title">
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
        </div>
    </div>
}


<script type="text/javascript">
    $('form input[type=submit]').click(function () {
        tinyMCE.triggerSave();
    });
</script>
