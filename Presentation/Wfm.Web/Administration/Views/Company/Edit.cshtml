@model CompanyBasicInformation

@{
    //page title
    //ViewBag.Title = T("Admin.Companies.Company.EditCompanyDetails").Text;
}

@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "company-form" }))
{
    <div class="section-header">
        <div class="title">
            @T("Admin.Companies.Company.EditCompanyDetails") @Html.ActionLink("(" + T("Admin.Companies.Company.BackToList") + ")", "Index")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
            @Html.ActionLink(T("Admin.Common.Cancel").Text, "Details", new { guid=Model.CompanyGuid}, new { @class = "k-button" })
        </div>
    </div>

    @Html.Partial("_CreateOrUpdate", Model)
}
