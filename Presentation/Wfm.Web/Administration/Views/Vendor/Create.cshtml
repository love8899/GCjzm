@model FranchiseModel

@{
    //page title
    //ViewBag.Title = T("Admin.Vendors.Vendor.AddNew").Text;
}

@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "vendor-form" }))
{

    <div class="section-header">
        <div class="title">
            @T("Common.Vendor") @Html.ActionLink("(" + T("Admin.Vendors.Vendor.BackToList") + ")", "Index")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
        </div>
    </div>

    @Html.Partial("_CreateOrUpdate", Model)
}
