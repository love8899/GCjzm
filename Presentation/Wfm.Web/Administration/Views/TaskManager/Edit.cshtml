@model ScheduleTaskModel

@{
    //page title
    ViewBag.Title = T("Admin.Configuration.ScheduleTask.EditScheduleTaskDetails").Text;
}

@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "scheduletasks-form" }))
{
    <div class="section-header">
        <div class="title">
            @T("Admin.Configuration.ScheduleTask.EditScheduleTaskDetails") @Html.ActionLink("(" + T("Admin.Configuration.ScheduleTask.BackToList") + ")", "Index")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
        </div>
    </div>

    @Html.Partial("_CreateOrUpdate", Model)
}
